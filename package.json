    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/supertest": "^6.0.3",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.14.0",
    "supertest": "^7.1.4",
